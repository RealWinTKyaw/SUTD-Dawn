<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.sutddawn.ui.search.Search"
        android:label="Search"
        tools:layout="@layout/activity_search" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.sutddawn.ui.marketplace.Group_View"
        android:label="Home"
        tools:layout="@layout/marketplace_group_view" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.sutddawn.backups.UserProfileB"
        android:label="Profile"
        tools:layout="@layout/activity_user_profile" />
</navigation>